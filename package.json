{
  "name": "@shaes/mui-mas",
  "version": "0.0.1",
  "description": "The SHAES Farm UI component library based on MUI",
  "author": "SHAES Farm <npm@shaes.farm> (https://github.com/shaes-farm)",
  "license": "GPL-3.0-or-later",
  "main": "./build/dist/index.js",
  "types": "./build/dist/index.d.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "env NODE_ENV=production node ./build/dist/index.js",
    "build": "tsc",
    "dev": "env DEBUG='shaes:*' nodemon ./build/dist/index.js",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint src/**/*.ts[x]",
    "lint:ts": "tsc --noEmit --strict",
    "verify": "npm run lint && npm run lint:ts && npm test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shaes-farm/mui-mas.git"
  },
  "keywords": [
    "UI",
    "React",
    "Material UI",
    "Component Library"
  ],
  "bugs": {
    "url": "https://github.com/shaes-farm/mui-mas/issues"
  },
  "homepage": "https://github.com/shaes-farm/mui-mas#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.22.2",
    "@babel/preset-react": "^7.22.3",
    "@babel/preset-typescript": "^7.21.5",
    "@storybook/addon-essentials": "^7.0.27",
    "@storybook/addon-interactions": "^7.0.27",
    "@storybook/addon-links": "^7.0.27",
    "@storybook/addon-styling": "^1.3.4",
    "@storybook/blocks": "^7.0.27",
    "@storybook/nextjs": "^7.0.27",
    "@storybook/react": "^7.0.27",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/chance": "^1.1.3",
    "@types/jest": "^29.5.1",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "babel-jest": "^29.5.0",
    "chance": "^1.1.11",
    "eslint": "^8.44.0",
    "eslint-config-standard-with-typescript": "^36.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-material-ui": "^1.0.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.12",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-extended": "^4.0.0",
    "storybook": "^7.0.27",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.9",
    "@mui/lab": "^5.0.0-alpha.135",
    "@mui/material": "^5.10.12",
    "@supabase/auth-helpers-react": "^0.4.0",
    "@supabase/supabase-js": "^2.26.0",
    "clsx": "^1.2.1",
    "next": "^13.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
