{
  "name": "@shaes-farm/mui-mas",
  "version": "0.0.5",
  "description": "The SHAES Farm UI component library based on MUI",
  "author": "SHAES Farm <npm@shaes.farm> (https://github.com/shaes-farm)",
  "license": "GPL-3.0-or-later",
  "main": "./build/dist/index.js",
  "types": "./build/dist/index.d.js",
  "directories": {
    "doc": "docs"
  },
  "engines": {
    "node": "^16 || ^18"
  },
  "scripts": {
    "start": "env NODE_ENV=production node ./build/dist/index.js",
    "build": "npm run verify && tsc --verbose",
    "clean": "tsc --clean",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint src/**/*.ts[x]",
    "lint:ts": "tsc --noEmit --strict",
    "verify": "npm run lint && npm run lint:ts && npm test",
    "storybook": "env STORYBOOK_DISABLE_TELEMETRY=1 storybook dev -p 6006",
    "build-storybook": "env STORYBOOK_DISABLE_TELEMETRY=1 storybook build",
    "prepublish": "npm run verify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shaes-farm/mui-mas.git"
  },
  "keywords": [
    "UI",
    "React",
    "Material UI",
    "Component Library"
  ],
  "bugs": {
    "url": "https://github.com/shaes-farm/mui-mas/issues"
  },
  "homepage": "https://github.com/shaes-farm/mui-mas#readme",
  "peerDependencies": {
    "@mui/material": "^5.14.1",
    "next": "13.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.22.2",
    "@babel/preset-react": "7.22.3",
    "@babel/preset-typescript": "7.21.5",
    "@faker-js/faker": "^8.0.2",
    "@storybook/addon-essentials": "7.2.3",
    "@storybook/addon-interactions": "7.2.3",
    "@storybook/addon-links": "7.2.3",
    "@storybook/addon-mdx-gfm": "7.2.3",
    "@storybook/addon-styling": "1.3.6",
    "@storybook/blocks": "7.2.3",
    "@storybook/nextjs": "7.2.3",
    "@storybook/react": "7.2.3",
    "@storybook/testing-library": "0.2.0",
    "@swc/helpers": "^0.5.1",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.5.1",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "babel-jest": "29.5.0",
    "eslint": "8.44.0",
    "eslint-config-standard-with-typescript": "36.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-material-ui": "1.0.1",
    "eslint-plugin-n": "15.7.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-storybook": "0.6.13",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "jest-extended": "4.0.0",
    "nodemon": "3.0.1",
    "storybook": "7.2.3",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@fontsource/material-icons": "5.0.7",
    "@fontsource/roboto": "5.0.6",
    "@mui/icons-material": "5.14.3",
    "@mui/lab": "5.0.0-alpha.135",
    "@mui/material": "^5.14.4",
    "@supabase/auth-helpers-react": "0.4.2",
    "@supabase/supabase-js": "2.32.0",
    "clsx": "1.2.1",
    "next": "13.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
